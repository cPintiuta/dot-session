{
  "name": "dotkernel/dot-session",
  "type": "library",
  "description": "DotKernel session component extending and customizing zend-session",
  "license": "MIT",
  "authors": [
    {
      "name": "DotKernel Team",
      "email": "team@dotkernel.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-session": "^2.8",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "zendframework/zend-stdlib": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Session\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev",
      "dev-develop": "3.1-dev"
    }
  }
}
